@using Domain.Entity
@using WebApp.Model.GenericMvc


@model TableViewModel

@{
    var descriptor = TableBuilder<EntityDisplay>.CreateNew()
        .AddColumn(x => x.Name)
        .AddColumn(x => x.Acronym)
        .AddColumn(x => x.IsSupplier)
        .AddColumn(x => x.VatNumber)
        .AddColumn(x => x.TaxNumber)
        .AddColumn(x => x.Type)
        .Build();
}

@await Html.TableAsync(Model, descriptor)

