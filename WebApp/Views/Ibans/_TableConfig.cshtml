@using Domain.Iban
@using WebApp.Model.GenericMvc


@model TableViewModel

@{
    var descriptor = TableBuilder<IbanDisplay>.CreateNew()
        .AddColumn(x => x.IbanStr)
        .AddColumn(x => x.Bank)
        .AddColumn(x => x.Currency)
        .AddColumn(x => x.Swift)
        .Build();
}

@await Html.TableAsync(Model, descriptor)

