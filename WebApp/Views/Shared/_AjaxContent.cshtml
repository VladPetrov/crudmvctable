@model string

@{
    var id = Guid.NewGuid().ToString();
}

<div id="container-@id">

</div>


<script>
    $.ajax({
        type: "GET",
        url: '@Model',
        data: '',
        contentType: "application/json; charset=utf-8",
        dataType: "HTML",
        success: response =>
        {
            $(`#container-@id`).html(response);
        }
    });
</script>